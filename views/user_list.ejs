<html lang="en-gb">
  <%- include('_head') -%> <%- include('_header') -%>
  <body>
    <div id="current-page">
    <h1>Users</h1>
    <ol>
      <% results.forEach(function(user) {%>
        <li>
          <div style="position: relative; left: 0; top: 0;">
          <a href="users/<%= user.id %>"> <%= user.name %> </a>
          <img class="cover-userlist" src="<%= user.coverImageUrl %>"/>
          <img class="profile-userlist" src="<%= user.profileImageUrl %>"/>
          <div class="after-cover"> 
            <%= user.username %>, <%= user.email %>
          </div>
         
        </div>
        </li>
      <% }); %>
    </ol>
    <%if (next) { %>
    <a href="<%= next %>">Next</a>
    <% } %> <%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
    <% } %>
  </div>
  </body>
  <%- include('_footer') -%>
</html>
